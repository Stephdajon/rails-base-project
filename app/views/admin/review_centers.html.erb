<div class="teacher-img d-flex justify-content-center"><%= image_tag "rc-list.png"%></div>
<div class="container ">
  <p>
    <a class="btn btn-outline-success d-block mt-3" data-bs-toggle="collapse" href="#collapseExample" role="button" aria-expanded="false" aria-controls="collapseExample">
      Review Center's List <i class="fas fa-caret-down"></i>
    </a>
    <a class="btn btn-outline-warning d-block mt-3" data-bs-toggle="collapse" href="#collapse2Example" role="button" aria-expanded="false" aria-controls="collapseExample">
      Pending Review Center <i class="fas fa-caret-down"></i>
    </a>
  </p>
  <div class="collapse" id="collapseExample">
    <div class="card card-body">
      <%= render "admin/review_center_list" %>
    </div>
  </div>
  <div class="collapse" id="collapse2Example">
    <div class="card card-body">
      <%= render "admin/pending_rc" %>
    </div>
  </div>
  <%= link_to "Back", admin_path, class: 'w-20 btn btn-lg btn-secondary mt-2' %>
</div>
